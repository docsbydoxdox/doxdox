            "param": [
                {
                    "name": "updated",
                    "isOptional": false,
                    "types": [
                        "Object"
                    ],
                    "description": "Additional options as key-value pairs."
                }
            ],
            "return": [
                {
                    "types": [
                        "Object"
                    ],
                    "description": "Default set of options."
                }
            ],
            "private": []
        }
    }
]