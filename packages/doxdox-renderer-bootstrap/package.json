{
  "name": "doxdox-renderer-bootstrap",
  "description": "Bootstrap renderer for doxdox.",
  "version": "4.0.0-preview.14",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^14.15.0 || ^16.10.0 || >=18.0.0"
  },
  "dependencies": {
    "highlight.js": "11.6.0",
    "markdown-it": "13.0.1",
    "markdown-table": "3.0.2"
  },
  "peerDependencies": {
    "doxdox-core": "4.0.0-preview.14"
  },
  "peerDependenciesMeta": {
    "doxdox-core": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/jest": "29.0.0",
    "@types/markdown-it": "12.2.3",
    "@types/node": "18.7.16",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "eslint": "8.23.0",
    "jest": "29.0.2",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.0-next.1",
    "ts-jest-resolver": "2.0.0",
    "typescript": "4.8.3"
  },
  "scripts": {
    "test": "npx cross-env@7 NODE_OPTIONS=--experimental-vm-modules jest --runInBand --passWithNoTests",
    "test-update": "npx cross-env@7 NODE_OPTIONS=--experimental-vm-modules jest --runInBand --passWithNoTests --updateSnapshot",
    "build": "rimraf dist/ && tsc",
    "lint": "eslint 'src/**/*.ts'"
  },
  "jest": {
    "collectCoverage": true,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "resolver": "ts-jest-resolver",
    "testRegex": ".*.test.ts$"
  },
  "keywords": [
    "doxdox",
    "documentation",
    "bootstrap"
  ],
  "authors": [
    {
      "name": "Scott Doxey",
      "email": "hello@scottdoxey.com",
      "homepage": "http://scottdoxey.com/"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/docsbydoxdox/doxdox",
  "repository": {
    "type": "git",
    "url": "git://github.com/docsbydoxdox/doxdox.git"
  }
}
