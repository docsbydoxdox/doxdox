{
  "name": "doxdox",
  "description": "Main package for doxdox.",
  "version": "4.0.0-preview.8",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "dependencies": {
    "doxdox-cli": "4.0.0-preview.8",
    "doxdox-core": "4.0.0-preview.8",
    "doxdox-parser-jsdoc": "4.0.0-preview.8",
    "doxdox-renderer-bootstrap": "4.0.0-preview.8",
    "doxdox-renderer-json": "4.0.0-preview.8",
    "doxdox-renderer-markdown": "4.0.0-preview.8"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "eslint": "8.9.0",
    "typescript": "4.5.5"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "lint": "eslint 'src/**/*.ts'"
  },
  "jest": {
    "collectCoverage": true,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".*.test.ts$"
  },
  "keywords": [
    "doxdox",
    "documentation",
    "cli",
    "jsdoc",
    "bootstrap",
    "markdown",
    "json"
  ],
  "authors": [
    {
      "name": "Scott Doxey",
      "email": "hello@scottdoxey.com",
      "homepage": "http://scottdoxey.com/"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/docsbydoxdox/doxdox",
  "repository": {
    "type": "git",
    "url": "git://github.com/docsbydoxdox/doxdox.git"
  }
}
