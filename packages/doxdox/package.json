{
  "name": "doxdox",
  "description": "Main package for doxdox.",
  "version": "4.0.0-preview.16",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^14.15.0 || ^16.10.0 || >=18.0.0"
  },
  "dependencies": {
    "doxdox-cli": "4.0.0-preview.16",
    "doxdox-core": "4.0.0-preview.16",
    "doxdox-parser-jsdoc": "4.0.0-preview.16",
    "doxdox-renderer-bootstrap": "4.0.0-preview.16",
    "doxdox-renderer-json": "4.0.0-preview.16",
    "doxdox-renderer-markdown": "4.0.0-preview.16"
  },
  "devDependencies": {
    "@types/node": "18.8.2",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "eslint": "8.24.0",
    "rimraf": "3.0.2",
    "typescript": "4.8.4"
  },
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "lint": "eslint 'src/**/*.ts'"
  },
  "jest": {
    "collectCoverage": true,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "testRegex": ".*.test.ts$"
  },
  "keywords": [
    "doxdox",
    "documentation",
    "cli",
    "jsdoc",
    "bootstrap",
    "markdown",
    "json"
  ],
  "authors": [
    {
      "name": "Scott Doxey",
      "email": "hello@scottdoxey.com",
      "homepage": "http://scottdoxey.com/"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/docsbydoxdox/doxdox",
  "repository": {
    "type": "git",
    "url": "git://github.com/docsbydoxdox/doxdox.git"
  }
}
