{
  "name": "doxdox-renderer-dash",
  "description": "Dash renderer for doxdox.",
  "version": "4.0.0-preview.9",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "dependencies": {
    "adm-zip": "0.5.9",
    "better-sqlite3": "7.5.0",
    "highlight.js": "11.4.0",
    "markdown-it": "12.3.2",
    "markdown-table": "3.0.2",
    "temp": "0.9.4"
  },
  "peerDependencies": {
    "doxdox-core": "4.0.0-preview.9"
  },
  "peerDependenciesMeta": {
    "doxdox-core": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/better-sqlite3": "7.4.2",
    "@types/jest": "27.4.1",
    "@types/markdown-it": "12.2.3",
    "@types/node": "17.0.21",
    "@types/temp": "0.9.1",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "copyfiles": "2.4.1",
    "eslint": "8.10.0",
    "jest": "27.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --passWithNoTests",
    "build": "rimraf dist/ && tsc && copyfiles -u 1 src/resources/* dist/",
    "lint": "eslint 'src/**/*.ts'"
  },
  "jest": {
    "collectCoverage": true,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".*.test.ts$"
  },
  "keywords": [
    "doxdox",
    "documentation",
    "dash"
  ],
  "authors": [
    {
      "name": "Scott Doxey",
      "email": "hello@scottdoxey.com",
      "homepage": "http://scottdoxey.com/"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/docsbydoxdox/doxdox",
  "repository": {
    "type": "git",
    "url": "git://github.com/docsbydoxdox/doxdox.git"
  }
}
