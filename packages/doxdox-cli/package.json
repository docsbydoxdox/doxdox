{
  "name": "doxdox-cli",
  "description": "CLI package for doxdox.",
  "version": "4.0.0-preview.4",
  "bin": {
    "doxdox": "./dist/src/index.js"
  },
  "exports": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "dependencies": {
    "doxdox-core": "4.0.0-preview.4",
    "doxdox-parser-jsdoc": "4.0.0-preview.4",
    "doxdox-renderer-bootstrap": "4.0.0-preview.4",
    "doxdox-renderer-json": "4.0.0-preview.4",
    "doxdox-renderer-markdown": "4.0.0-preview.4",
    "globby": "13.1.1",
    "import-from": "4.0.0",
    "parse-cmd-args": "4.0.0",
    "update-notifier": "5.1.0"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "17.0.18",
    "@types/update-notifier": "5.1.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --passWithNoTests",
    "build": "rm -rf dist/ && tsc && chmod +x ./dist/src/index.js",
    "link": "npm run build && npm link"
  },
  "keywords": [
    "doxdox",
    "documentation",
    "cli"
  ],
  "jest": {
    "collectCoverage": true,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "homepage": "https://github.com/docsbydoxdox/doxdox",
  "repository": {
    "type": "git",
    "url": "git://github.com/docsbydoxdox/doxdox.git"
  }
}
